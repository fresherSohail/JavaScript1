<html>
<head>
<script>

var a = [10, 1, 2, 3];

document.write(a + "<br>" + "<br>");
 
var b = a.some(checked);

document.write("Some function: "+ b + "<br>" + "<br>");
 
var c = a.every(checked);

document.write("Every function: "+ c);

function checked(age){
	return age>=10;
}

</script>
</head>
<body>
</html>
